{
  "name": "Night Mode",
  "description": "Protect your eyes at night",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"

  },

  "permissions": [
    "storage",
    "activeTab",
    "scripting"
  ],

  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "64": "icon64.png"

    }

  }

}